<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.huangjie.demo.ui.activity.MainActivity">

    <com.huangjie.demo.ui.view.GridViewPager
        android:id="@+id/grid_view_pager"
        android:layout_marginTop="@dimen/grid_section_top_margin"
        android:layout_gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/grid_section_height">

    </com.huangjie.demo.ui.view.GridViewPager>

</FrameLayout>
